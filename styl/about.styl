.about-top
    padding: 58px 0px
    .h1-about
        font-family: Inter;
        font-style: normal;
        font-weight: 500;
        font-size: 160px;
        line-height: 100%;
        margin: 0px 0px 80px;
        /* or 160px */
        letter-spacing: 0.03em;
        text-transform: uppercase;
        /* black */
        color: #202020;
.to-skroll
    display: flex;
    align-items: center;
.skroll-text 
    margin: 0px;
    font-family: Inter;
    font-style: normal;
    font-weight: normal;
    font-size: 20px;
    line-height: 24px;
    /* black */
    color: #202020;    
.skroll-text-wrap
    margin-right 32px;    

.what-about
    padding: 110px 0px  
    background: url('./../img/logoback.png');
    background-repeat: no-repeat
    background-size: 900px;
    background-position left 
    .big-header
        font-size: 54px;
        line-height: 70px;  

.services-row
    display: flex;
    width: 100%;
    // justify-content: flex-start;
    .medium_header
        max-width: 100%;
        margin-bottom 10px;
    .normal_text
        margin: 0px;    
    .only-left-2
        max-width: 37%;
        align-self: flex-start
    .only-right-2
        align-self: flex-end  
        max-width: 49%  
.services-row.right-row 
    justify-content: flex-end
    .small_header
        margin-bottom 22px;
    .big-header
        font-size: 30px;
        line-height: 39px;    

.story-about
    padding: 110px 0px;
    .big-header
        max-width: 59%
    .wrap-header
        border-bottom: 1px solid dark-color;
        padding-bottom 45px;  
        margin-bottom 35px;

.persons-wrap
    display: flex;
    flex-flow: wrap;
    justify-content: space-between;
    .person-item
        width: 33%;
        border-bottom: 1px solid dark-color;
        margin-bottom 35px;
    .person-img
        border-radius: 100%;
        width: 100%;
        overflow: hidden;
        img 
            display: block;
            width: 100%;
    .person-name
        font-family: inherit;
        font-style: normal;
        font-weight: normal;
        font-size: 24px;
        line-height: 130%;
        color: dark-color;
        span 
            font-weight: bold;
.choose-command
    padding: 80px 0px 453px;  
    .big-header
        max-width: 80%;
        line-height: 71px;

.rewiew-section
    max-width: 700px;
    display: flex
    align-self: flex-end
    float: right
    margin-top -75px;
    .owl-carousel
        width: 100%
        .rew-img
            margin-bottom 15px
        .rew-text
            font-family: inherit;
            font-style: normal;
            font-weight: normal;
            font-size: 24px;
            line-height: 130%;
            /* or 31px */
            letter-spacing: 0.03em;
            /* black */
            color: #202020;

.go-to-command
    padding: 100px 0px;
    .h2 
        max-width: 50%;
    .small_header
        font-weight: 400
        font-size: 20px;
        line-height: 130%;
        max-width: 25%
        

.go-to-form-wrap 
    display: flex;
    justify-content: space-between

